{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "start": "node -r ts-node/register server/index.ts",
    "build": "next build client",
    "server": "cross-env NODE_ENV=production node -r ts-node/register server/index.ts",
    "analyze": "cross-env BUNDLE_ANALYZE=both npm run build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser npm run build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint client/**/*.{ts,tsx} --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "^4.2.0",
    "@sentry/browser": "^5.7.0",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "accepts": "^1.3.7",
    "body-parser": "^1.19.0",
    "clsx": "^1.0.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.8.16",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "dotenv": "^8.1.0",
    "dotenv-webpack": "^1.7.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "formik": "^2.0.1-rc.13",
    "glob": "^7.1.4",
    "helmet": "^3.21.1",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "lokijs": "^1.5.7",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "next": "^9.1.1",
    "next-redux-wrapper": "^4.0.1",
    "nodemon": "^1.19.3",
    "numeral": "^2.0.6",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-intl": "^3.3.2",
    "react-redux": "^7.1.1",
    "react-window": "^1.8.5",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.3",
    "winston": "^3.2.1",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@storybook/addon-actions": "^5.2.3",
    "@storybook/addon-info": "^5.2.3",
    "@storybook/addon-knobs": "^5.2.3",
    "@storybook/addon-links": "^5.2.3",
    "@storybook/addons": "^5.2.3",
    "@storybook/react": "^5.2.3",
    "@svgr/webpack": "^4.3.3",
    "@testing-library/cypress": "^5.0.1",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "@types/accepts": "^1.3.5",
    "@types/body-parser": "^1.17.1",
    "@types/compression": "^1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/del": "^4.0.0",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.1",
    "@types/express-session": "^1.15.14",
    "@types/glob": "^7.1.1",
    "@types/helmet": "^0.0.44",
    "@types/intl": "^1.2.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.144",
    "@types/lokijs": "^1.5.2",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/next": "^8.0.6",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-intl": "^3.0.0",
    "@types/react-redux": "^7.1.4",
    "@types/redux-mock-store": "^1.0.1",
    "@types/yup": "^0.26.24",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "autoprefixer": "^9.6.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-react-intl": "^4.1.20",
    "coveralls": "^3.0.7",
    "cross-env": "^6.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.4.0",
    "lint-staged": "^9.4.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "redux-mock-store": "^1.5.3",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
