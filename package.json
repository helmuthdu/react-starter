{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "start": "node -r ts-node/register server/index.ts",
    "build": "next build client",
    "server": "cross-env NODE_ENV=production node -r ts-node/register server/index.ts",
    "analyze": "cross-env ANALYZE=true npm run build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint client/**/*.{ts,tsx} --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.0",
    "@sentry/react": "^5.27.1",
    "@zeit/next-typescript": "^1.1.1",
    "accepts": "^1.3.7",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.9.4",
    "debug": "^4.2.0",
    "del": "^6.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "glob": "^7.1.6",
    "helmet": "^4.1.1",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.1.0",
    "lodash-es": "^4.17.15",
    "lokijs": "^1.5.11",
    "method-override": "^3.0.0",
    "moize": "^5.4.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "next": "^10.0.0",
    "nodemon": "^2.0.6",
    "number-format.js": "^2.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.9.6",
    "react-intl": "^5.8.8",
    "react-query": "^2.25.2",
    "react-window": "^1.8.5",
    "rxjs": "^6.6.3",
    "sass": "^1.27.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-knobs": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addons": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "@storybook/theming": "^6.0.27",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@types/accepts": "^1.3.5",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/debug": "^4.1.5",
    "@types/del": "^4.0.0",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/glob": "^7.1.3",
    "@types/helmet": "^0.0.48",
    "@types/intl": "^1.2.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.163",
    "@types/lokijs": "^1.5.3",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.4",
    "@types/next": "^9.0.0",
    "@types/node": "^12.19.3",
    "@types/react": "^16.9.55",
    "@types/react-intl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.5.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.5.0",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
