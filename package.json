{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --coverage --watch",
    "test:coverage": "react-scripts test --coverage",
    "test:debug": "react-scripts test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@sentry/react": "^6.2.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.4",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "number-format.js": "^2.0.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.15.4",
    "react-hot-loader": "^4.13.0",
    "react-intl": "^5.12.5",
    "react-query": "^3.12.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-window": "^1.8.6",
    "recoil": "^0.1.2",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.1.20",
    "@storybook/addon-knobs": "^6.1.20",
    "@storybook/addon-links": "^6.1.20",
    "@storybook/addons": "^6.1.20",
    "@storybook/react": "^6.1.20",
    "@testing-library/cypress": "^7.0.4",
    "@testing-library/dom": "^7.29.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.3",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "babel-loader": "8.1.0",
    "coveralls": "^3.1.0",
    "cypress": "^6.5.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.1",
    "gh-pages": "^3.1.0",
    "husky": "^5.1.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.3",
    "react-snap": "^1.23.0",
    "sass": "^1.32.8",
    "typescript": "~4.2.2",
    "web-vitals": "^1.1.0",
    "webpack": "4.44.2",
    "workbox-background-sync": "^6.1.1",
    "workbox-broadcast-update": "^6.1.1",
    "workbox-cacheable-response": "^6.1.1",
    "workbox-core": "^6.1.1",
    "workbox-expiration": "^6.1.1",
    "workbox-google-analytics": "^6.1.1",
    "workbox-navigation-preload": "^6.1.1",
    "workbox-precaching": "^6.1.1",
    "workbox-range-requests": "^6.1.1",
    "workbox-routing": "^6.1.1",
    "workbox-strategies": "^6.1.1",
    "workbox-streams": "^6.1.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
