{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --coverage --watch",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "tslint --fix --format -c tslint.json src/**/*.{ts,tsx}",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-scripts test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "anchorate": "^1.2.3",
    "axios": "^0.18.0",
    "connected-react-router": "^6.2.2",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@craco/craco": "^3.4.1",
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-knobs": "^4.1.11",
    "@storybook/addon-links": "^4.1.11",
    "@storybook/addons": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/enzyme": "^3.1.16",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.13",
    "@types/lodash": "^4.14.120",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-redux": "^7.0.1",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.4",
    "coveralls": "^3.0.2",
    "cypress": "^3.1.5",
    "cypress-testing-library": "^2.3.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^8.1.3",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-hot-loader": "^4.6.5",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.8.1",
    "redux-mock-store": "^1.5.3",
    "storybook-readme": "^4.0.5",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-jest": "^1.0.5",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "3.3.1",
    "webpack": "4.19.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/**/*.styled.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!test/**/*.js",
      "!node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
