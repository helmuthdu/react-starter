{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "snowpack dev --reload",
    "build": "snowpack build",
    "test": "jest",
    "test:watch": "jest --coverage --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@sentry/react": "^6.15.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "history": "^5.1.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "number-format.js": "^2.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.20.4",
    "react-intl": "^5.22.0",
    "react-query": "^3.33.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "react-window": "^1.8.6",
    "recoil": "^0.5.2",
    "rxjs": "^7.4.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@snowpack/app-scripts-react": "^2.0.1",
    "@snowpack/plugin-dotenv": "^2.2.0",
    "@snowpack/plugin-react-refresh": "^2.5.0",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@snowpack/plugin-webpack": "^3.0.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/addons": "^6.4.0",
    "@storybook/react": "^6.4.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/snowpack-env": "^2.3.4",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "coveralls": "^3.1.1",
    "cypress": "^9.1.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-testing-library": "^5.0.0",
    "gh-pages": "^3.2.3",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-watch-typeahead": "^1.0.0",
    "postcss": "^8.4.3",
    "prettier": "^2.5.0",
    "react-snap": "^1.23.0",
    "sass": "^1.43.5",
    "serve": "^13.0.2",
    "snowpack": "^3.8.8",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.5.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
