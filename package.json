{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true npm run build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.0.7",
    "@sentry/react": "^6.16.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "next": "^12.0.7",
    "number-format.js": "^2.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.5",
    "react-intl": "^5.24.2",
    "react-query": "^3.34.7",
    "react-window": "^1.8.6",
    "recoil": "^0.5.2",
    "rxjs": "^7.5.1",
    "swr": "^1.1.2"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^6.4.10",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-links": "^6.4.10",
    "@storybook/addons": "^6.4.10",
    "@storybook/react": "^6.4.10",
    "@storybook/theming": "^6.4.10",
    "@svgr/webpack": "^6.1.2",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/intl": "^1.2.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.4.6",
    "coveralls": "^3.1.1",
    "cypress": "^9.2.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.0.3",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.7",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "mutationobserver-shim": "^0.3.7",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "sass": "^1.47.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
