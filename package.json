{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "next ./src",
    "server": "next start ./src",
    "build": "next build ./src",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@sentry/browser": "^5.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "next": "^8.1.0",
    "numeral": "^2.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "react-window": "^1.8.2",
    "redux": "^4.0.1",
    "redux-form": "^8.2.3",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.4",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-knobs": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@testing-library/cypress": "^4.0.0",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.133",
    "@types/next": "^8.0.5",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/redux-form": "^8.1.1",
    "@types/redux-mock-store": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "coveralls": "^3.0.3",
    "css-loader": "^1.0.1",
    "cypress": "^3.3.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.1.7",
    "node-sass": "^4.12.0",
    "prettier": "^1.17.1",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "typescript": "^3.5.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts?(x)",
      "!src/**/*.d.ts",
      "!src/**/*.stories.ts",
      "!src/**/*.styled.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!test/**/*.js",
      "!node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
