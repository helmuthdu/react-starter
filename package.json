{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:watch": "react-app-rewired test --env=jsdom --coverage --watch",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "test:debug": "react-app-rewired test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-app-rewired test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@sentry/browser": "^5.10.2",
    "clsx": "^1.0.4",
    "dayjs": "^1.8.17",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "moize": "^5.4.4",
    "number-format.js": "^2.0.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^3.29.2",
    "react-hot-loader": "^4.12.18",
    "react-intl": "^3.9.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.5",
    "rxjs": "^6.5.3",
    "swr": "^0.1.13"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-links": "^5.2.8",
    "@storybook/addons": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/dom": "^6.11.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "babel-loader": "8.0.6",
    "coveralls": "^3.0.9",
    "cypress": "^3.8.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-plugin-cypress": "^2.8.0",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^9.5.0",
    "mutationobserver-shim": "^0.3.3",
    "prettier": "^1.19.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "^3.3.0",
    "react-snap": "^1.23.0",
    "sass": "^1.23.7",
    "typescript": "^3.7.3",
    "webpack": "4.41.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
