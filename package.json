{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true npm run build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.1.6",
    "@sentry/react": "^7.0.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.11.2",
    "lodash": "^4.17.21",
    "next": "^12.1.6",
    "number-format.js": "^2.0.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.3",
    "react-intl": "^6.0.3",
    "react-query": "^3.39.1",
    "react-window": "^1.8.7",
    "recoil": "^0.7.2",
    "rxjs": "^7.5.5",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@svgr/webpack": "^6.2.1",
    "@testing-library/react": "^13.3.0",
    "@types/intl": "^1.2.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^28.1.0",
    "coveralls": "^3.1.1",
    "cypress": "^9.7.0",
    "eslint": "^8.16.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "gh-pages": "^4.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^1.1.0",
    "mutationobserver-shim": "^0.3.7",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^4.0.0",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
