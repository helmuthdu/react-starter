{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --coverage --watch",
    "test:coverage": "react-scripts test --coverage",
    "test:debug": "react-scripts test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@sentry/react": "^6.14.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.7",
    "history": "^5.1.0",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "moize": "^6.1.0",
    "number-format.js": "^2.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.19.1",
    "react-hot-loader": "^4.13.0",
    "react-intl": "^5.21.1",
    "react-query": "^3.32.1",
    "react-router": "^6.0.1",
    "react-router-dom": "^6.0.1",
    "react-window": "^1.8.6",
    "rxjs": "^7.4.0",
    "swr": "^1.0.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/dom": "^8.11.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "8.1.0",
    "coveralls": "^3.1.1",
    "cypress": "^8.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.0.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.4.1",
    "postcss": "^8.3.11",
    "react-scripts": "4.0.3",
    "react-snap": "^1.23.0",
    "sass": "^1.43.4",
    "stylelint": "^14.0.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "typescript": "^4.4.4",
    "web-vitals": "^2.1.2",
    "webpack": "4.44.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
