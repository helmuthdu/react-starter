{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next ./src",
    "start": "next start ./src",
    "build": "next build ./src",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "tslint --fix --format -c tslint.json src/**/*.{ts,tsx}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "next": "^8.0.3",
    "numeral": "^2.0.6",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-knobs": "^5.0.3",
    "@storybook/addon-links": "^5.0.3",
    "@storybook/addons": "^5.0.3",
    "@storybook/react": "^5.0.3",
    "@types/enzyme": "^3.9.1",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/next": "^8.0.3",
    "@types/react": "^16.8.8",
    "@types/react-dom": "^16.8.3",
    "@types/react-redux": "^7.0.5",
    "@types/redux-mock-store": "^1.0.0",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-jest": "^24.5.0",
    "coveralls": "^3.0.3",
    "css-loader": "^1.0.1",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^2.3.6",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.2.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-test-renderer": "^16.8.5",
    "redux-mock-store": "^1.5.3",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.14.0",
    "tslint-jest": "^1.0.5",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "3.3.3333"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/**/*.styled.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!test/**/*.js",
      "!node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
