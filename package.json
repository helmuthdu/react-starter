{
  "name": "react-starter",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --coverage --watch",
    "test:coverage": "react-scripts test --coverage",
    "test:debug": "react-scripts test --inspect-brk --runInBand --watch",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint --fix src/**/*.{css,scss}",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "@sentry/react": "^7.106.1",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "history": "^5.3.0",
    "isomorphic-unfetch": "^4.0.2",
    "number-format.js": "^2.0.9",
    "radash": "^12.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.51.0",
    "react-intl": "^6.6.2",
    "react-query": "^4.0.0",
    "react-redux": "^9.1.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "react-window": "^1.8.10",
    "redux-logger": "^3.0.6",
    "rxjs": "^7.8.1",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.26",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.21",
    "@types/redux-logger": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "gh-pages": "^6.1.1",
    "lefthook": "^1.6.5",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "sass": "^1.71.1",
    "stylelint": "^16.2.1",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "^5.4.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
