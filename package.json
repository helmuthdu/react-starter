{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:watch": "react-app-rewired test --coverage --watch",
    "test:coverage": "react-app-rewired test --coverage",
    "test:debug": "react-app-rewired test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-app-rewired test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "@sentry/react": "^5.22.3",
    "clsx": "^1.1.1",
    "dayjs": "^1.8.35",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.20",
    "moize": "^5.4.7",
    "number-format.js": "^2.0.9",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.7.2",
    "react-hot-loader": "^4.12.21",
    "react-intl": "^5.8.0",
    "react-query": "^2.15.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-window": "^1.8.5",
    "rxjs": "^6.6.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.4.5",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@testing-library/cypress": "^7.0.0",
    "@testing-library/dom": "^7.24.1",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/user-event": "^12.1.3",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.1.0",
    "coveralls": "^3.1.0",
    "cypress": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.5.1",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.5.0",
    "lint-staged": "^10.3.0",
    "mutationobserver-shim": "^0.3.7",
    "prettier": "^2.1.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "^3.4.3",
    "react-snap": "^1.23.0",
    "sass": "^1.26.10",
    "typescript": "^3.9.7",
    "webpack": "4.42.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
