{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test:watch": "react-app-rewired test --coverage --watch",
    "test:coverage": "react-app-rewired test --coverage",
    "test:debug": "react-app-rewired test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint ./src/**/*.{ts,tsx} --fix",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-app-rewired test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "@sentry/browser": "^5.12.1",
    "clsx": "^1.1.0",
    "dayjs": "^1.8.20",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "moize": "^5.4.5",
    "number-format.js": "^2.0.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^4.9.6",
    "react-hot-loader": "^4.12.19",
    "react-intl": "^3.12.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.5",
    "rxjs": "^6.5.4",
    "swr": "^0.1.17"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-info": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.13",
    "@testing-library/cypress": "^5.3.0",
    "@testing-library/dom": "^6.12.2",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.27",
    "@types/react": "^16.9.20",
    "@types/react-dom": "^16.9.5",
    "@types/react-router": "^5.1.4",
    "@types/react-router-dom": "^5.1.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "coveralls": "^3.0.9",
    "cypress": "^4.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-cypress": "^2.10.2",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "jest": "24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.0.7",
    "mutationobserver-shim": "^0.3.3",
    "prettier": "^1.19.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "^3.4.0",
    "react-snap": "^1.23.0",
    "sass": "^1.25.0",
    "typescript": "^3.7.5",
    "webpack": "4.41.5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
