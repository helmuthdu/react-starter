{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "start": "node -r ts-node/register server/index.ts",
    "build": "next build client",
    "server": "cross-env NODE_ENV=production node -r ts-node/register server/index.ts",
    "analyze": "cross-env ANALYZE=true npm run build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint client/**/*.{ts,tsx} --fix",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@formatjs/intl-relativetimeformat": "^4.5.9",
    "@sentry/browser": "^5.12.4",
    "@next/bundle-analyzer": "^9.2.2",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "accepts": "^1.3.7",
    "body-parser": "^1.19.0",
    "clsx": "^1.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.8.21",
    "debug": "^4.1.1",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^1.7.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "glob": "^7.1.6",
    "helmet": "^3.21.3",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash-es": "^4.17.15",
    "lokijs": "^1.5.8",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "next": "^9.2.2",
    "nodemon": "^2.0.2",
    "number-format.js": "^2.0.9",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^4.9.8",
    "react-intl": "^3.12.0",
    "react-window": "^1.8.5",
    "rxjs": "^6.5.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@cypress/webpack-preprocessor": "^4.1.2",
    "@storybook/addon-actions": "^5.3.14",
    "@storybook/addon-info": "^5.3.14",
    "@storybook/addon-knobs": "^5.3.14",
    "@storybook/addon-links": "^5.3.14",
    "@storybook/addons": "^5.3.14",
    "@storybook/react": "^5.3.14",
    "@svgr/webpack": "^5.2.0",
    "@testing-library/cypress": "^5.3.0",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "@types/accepts": "^1.3.5",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/debug": "^4.1.5",
    "@types/del": "^4.0.0",
    "@types/errorhandler": "^1.5.0",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/glob": "^7.1.1",
    "@types/helmet": "^0.0.45",
    "@types/intl": "^1.2.0",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/lokijs": "^1.5.3",
    "@types/method-override": "^0.0.31",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.4.2",
    "@types/next": "^9.0.0",
    "@types/react": "^16.9.23",
    "@types/react-intl": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-react-intl": "^5.1.18",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-fetch-mock": "^3.0.1",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.0.8",
    "mutationobserver-shim": "^0.3.3",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
