{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:watch": "react-app-rewired test --env=jsdom --coverage --watch",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "test:debug": "react-app-rewired test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint ./src --fix",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-app-rewired test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@sentry/browser": "^5.7.1",
    "@zeit/swr": "0.0.3",
    "clsx": "^1.0.4",
    "dayjs": "^1.8.16",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "moize": "^5.4.4",
    "number-format.js": "^2.0.9",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^3.26.2",
    "react-hot-loader": "^4.12.15",
    "react-intl": "^3.4.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-window": "^1.8.5",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/cypress": "^5.0.2",
    "@testing-library/dom": "^6.9.0",
    "@testing-library/jest-dom": "^4.2.0",
    "@testing-library/react": "^9.3.0",
    "@types/jest": "^24.0.20",
    "@types/lodash": "^4.14.144",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router": "^5.1.2",
    "@types/react-router-dom": "^5.1.0",
    "babel-loader": "8.0.6",
    "coveralls": "^3.0.7",
    "cypress": "^3.5.0",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-cypress": "^2.7.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.9",
    "jest": "24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.4.0",
    "lint-staged": "^9.4.2",
    "mutationobserver-shim": "^0.3.3",
    "prettier": "^1.18.2",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.4",
    "react-scripts": "^3.2.0",
    "react-snap": "^1.23.0",
    "sass": "^1.23.1",
    "typescript": "^3.6.4",
    "webpack": "4.41.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
