{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom --coverage --watch",
    "test:coverage": "react-scripts test --env=jsdom --coverage",
    "test:debug": "react-scripts --inspect-brk test --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "flow": "flow",
    "lint": "jest --config jest.eslint.config.js",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-scripts test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "anchorate": "^1.2.3",
    "axios": "^0.18.0",
    "connected-react-router": "^6.1.0",
    "history": "^4.7.2",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "numeral": "^2.0.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.3.0",
    "rxjs": "^6.3.3",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.2",
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-knobs": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "axios-mock-adapter": "^1.16.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.4",
    "coveralls": "^3.0.2",
    "cypress": "^3.1.4",
    "cypress-testing-library": "^2.3.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.6.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-jest": "^2.1.1",
    "eslint-plugin-cypress": "^2.2.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "flow-bin": "^0.89.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jest-runner-eslint": "^0.7.1",
    "jest-watch-typeahead": "^0.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react-hot-loader": "^4.6.3",
    "react-scripts": "^2.1.3",
    "react-test-renderer": "^16.7.0",
    "redux-mock-store": "^1.5.3",
    "storybook-readme": "^4.0.2",
    "webpack": "4.19.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.stories.js",
      "!src/**/*.styled.js",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!test/**/*.js",
      "!node_modules/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
