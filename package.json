{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true npm run build",
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom --coverage --watch",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:debug": "jest --inspect-brk --runInBand --watch",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "coveralls": "jest --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^14.0.3",
    "@sentry/react": "~7.84.0",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.10",
    "next": "^14.0.3",
    "number-format.js": "^2.0.9",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-intl": "^6.5.5",
    "react-query": "^3.39.3",
    "react-window": "^1.8.10",
    "recoil": "^0.7.7",
    "rxjs": "^7.8.1",
    "swr": "^2.2.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/react": "^14.1.2",
    "@types/intl": "^1.2.2",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.10.1",
    "@types/react": "^18.2.39",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "babel-jest": "^29.7.0",
    "coveralls": "^3.1.1",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "gh-pages": "^6.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "lefthook": "^1.5.5",
    "mutationobserver-shim": "^0.3.7",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
