{
  "name": "react-starter",
  "homepage": "http://helmuthdu.github.io/react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:watch": "react-app-rewired test --env=jsdom --coverage --watch",
    "test:coverage": "react-app-rewired test --env=jsdom --coverage",
    "test:debug": "react-app-rewired test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "eslint ./src --fix",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-app-rewired test --env=jsdom --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@sentry/browser": "^5.4.0",
    "anchorate": "^1.2.3",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.4.0",
    "formik": "^1.5.7",
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.11.0",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-window": "^1.8.2",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.0",
    "@storybook/addon-actions": "^5.1.3",
    "@storybook/addon-info": "^5.1.3",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-links": "^5.1.3",
    "@storybook/addons": "^5.1.3",
    "@storybook/react": "^5.1.3",
    "@testing-library/cypress": "^4.0.3",
    "@testing-library/react": "^8.0.1",
    "@types/classnames": "^2.2.8",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-mock-store": "^1.0.1",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__react": "^4.0.2",
    "@types/yup": "^0.26.16",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "8.0.5",
    "coveralls": "^3.0.4",
    "cypress": "^3.3.1",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.4.1",
    "jest": "24.7.1",
    "jest-dom": "^3.5.0",
    "jest-environment-jsdom-fourteen": "^0.1.0",
    "jest-transform-stub": "^2.0.0",
    "jest-watch-typeahead": "^0.3.1",
    "lint-staged": "^8.2.0",
    "node-sass": "^4.12.0",
    "prettier": "^1.18.2",
    "react-app-polyfill": "^1.0.1",
    "react-app-rewire-hot-loader": "^2.0.1",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "^3.0.1",
    "react-snap": "^1.23.0",
    "redux-mock-store": "^1.5.3",
    "typescript": "^3.5.1",
    "webpack": "4.29.6"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
