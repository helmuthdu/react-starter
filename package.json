{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --coverage --watch",
    "test:coverage": "react-scripts test --coverage",
    "test:debug": "react-scripts test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "build-storybook -s public",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@sentry/react": "^6.11.0",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.6",
    "isomorphic-unfetch": "^3.1.0",
    "lodash": "^4.17.21",
    "moize": "^6.1.0",
    "number-format.js": "^2.0.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.14.1",
    "react-hot-loader": "^4.13.0",
    "react-intl": "^5.20.10",
    "react-query": "^3.21.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.2.1",
    "react-window": "^1.8.6",
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.6",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addons": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/dom": "^8.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14.17.12",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "babel-loader": "8.1.0",
    "coveralls": "^3.1.1",
    "cypress": "^8.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-testing-library": "^4.12.0",
    "gh-pages": "^3.2.3",
    "prettier": "^2.3.2",
    "react-scripts": "4.0.3",
    "react-snap": "^1.23.0",
    "sass": "^1.38.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.20.1",
    "typescript": "~4.3.5",
    "web-vitals": "^2.1.0",
    "webpack": "4.44.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
