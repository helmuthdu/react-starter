{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --coverage --watch",
    "test:coverage": "react-scripts test --coverage",
    "test:debug": "react-scripts test --inspect-brk --runInBand --watch",
    "test:e2e": "cypress open",
    "test:e2e:run": "cypress run --headed",
    "lint": "npm run lint:script && npm run lint:style",
    "lint:script": "eslint ./src --ext .jsx,.js,.ts,.tsx --fix --ignore-path .gitignore",
    "lint:style": "stylelint src/**/*.{css,scss}",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "postbuild": "react-snap",
    "coveralls": "react-scripts test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@sentry/react": "^7.45.0",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "history": "^5.3.0",
    "isomorphic-unfetch": "^4.0.2",
    "lodash": "^4.17.21",
    "number-format.js": "^2.0.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.43.8",
    "react-intl": "^6.3.2",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router": "^6.9.0",
    "react-router-dom": "^6.9.0",
    "react-window": "^1.8.8",
    "redux-logger": "^3.0.6",
    "rxjs": "^7.8.0",
    "swr": "^2.1.1"
  },
  "devDependencies": {
    "@arkweid/lefthook": "^0.7.7",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "coveralls": "^3.1.1",
    "cypress": "^12.8.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testing-library": "^5.10.2",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "react-scripts": "5.0.1",
    "react-snap": "^1.23.0",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^7.0.1",
    "typescript": "^5.0.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
